{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "paths": {
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/nest-postgre": [
        "libs/nest-postgre/src"
      ],
      "@app/nest-postgre/*": [
        "libs/nest-postgre/src/*"
      ],
      "@app/amqp-contracts": [
        "libs/amqp-contracts/src"
      ],
      "@app/amqp-contracts/*": [
        "libs/amqp-contracts/src/*"
      ],
      "@app/graphql-lib": [
        "libs/graphql-lib/src"
      ],
      "@app/graphql-lib/*": [
        "libs/graphql-lib/src/*"
      ]
    }
  }
}